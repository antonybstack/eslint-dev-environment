{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "no-var": "error",
        "semi": "error",
        "no-multi-spaces": "error",
        "space-in-parens": "error",
        "no-multiple-empty-lines": "error",
        "prefer-const": "error",
        "no-use-before-define": "error",
        "quotes": ["error", "double"],
        "no-unused-vars": "off"
    }
}
